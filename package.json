{
  "name": "FnO",
  "version": "0.1.1",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=dev next dev -p 3010",
    "build": "next build",
    "start": "next start -p 3010",
    "lint": "next lint",
    "format": "prettier --write \"{src,test}/**/*.{tsx,ts}\"",
    "svgs": "svgr --ext tsx -d ./src/assets/svg ./src/assets/svg/src",
    "devInstall": "NODE_ENV=development yarn install",
    "clean:install": "rm -rf node_modules && rm -rf .next && yarn install"
  },
  "dependencies": {
    "@svgr/cli": "^6.5.1",
    "@types/axios": "^0.14.0",
    "@types/cookie": "^0.5.1",
    "@types/node": "18.13.0",
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.8",
    "@types/socket.io-client": "^3.0.0",
    "axios": "^1.1.3",
    "cookie": "^0.5.0",
    "cookies-next": "^2.1.1",
    "eslint": "8.26.0",
    "eslint-config-next": "13.0.0",
    "next": "13.1.6 ",
    "prettier": "^2.7.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.11.0",
    "sharp": "^0.31.2",
    "socket.io-client": "^4.5.3",
    "typescript": "4.9.5"
  }
}
